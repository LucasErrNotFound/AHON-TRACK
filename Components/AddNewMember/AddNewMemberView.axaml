														HorizontalAlignment="Left">
														<TextBlock
															Text="Cash"
															FontSize="16"
															FontWeight="SemiBold"
															Foreground="{DynamicResource WarningColor}" />
													</Border>
												</StackPanel>
											</StackPanel>
											<StackPanel 
												Grid.Column="1" 
												Orientation="Vertical" 
												Margin="0,30"
												IsVisible="{CompiledBinding IsReferenceNumberVisibleInReceipt}">
												<TextBlock
													Classes="h4"
													Text="Reference No."
													TextAlignment="Right"
													TextWrapping="WrapWithOverflow"
													HorizontalAlignment="Right"
													VerticalAlignment="Center" />
												<TextBlock
													Classes="Muted"
													Text="{CompiledBinding ReferenceNumber}"
													TextAlignment="Right"
													Width="160"
													TextWrapping="WrapWithOverflow"
													HorizontalAlignment="Right"
													VerticalAlignment="Center" />
											</StackPanel>
										</Grid>
										<Separator Background="{StaticResource ForegroundColor}" />
										<Grid RowDefinitions="Auto" ColumnDefinitions="* *" IsVisible="{CompiledBinding IsPaymentPossible}">
											<!-- Left Column -->
											<StackPanel Grid.Column="0" Spacing="0">
												<StackPanel Orientation="Horizontal" Margin="0,30" Spacing="15">
													<Svg
														Path="avares://AHON_TRACK/Assets/CheckInOutView/calendar-icon.svg"
														Width="24"
														Height="24"
														HorizontalAlignment="Left"
														VerticalAlignment="Center" />
													<TextBlock
														Classes="Muted"
														Text="Transaction Details"
														VerticalAlignment="Center" />
												</StackPanel>
												<StackPanel Orientation="Vertical" Margin="40,-25,0,20" Spacing="10">
													<StackPanel Orientation="Vertical" Spacing="5" Margin="0,15">
														<TextBlock
															Text="Date:"
															Classes="h5" />
														<TextBlock
															Text="Time:"
															Classes="h5" />
													</StackPanel>
												</StackPanel>
											</StackPanel>

											<!-- Right Column -->
											<StackPanel Grid.Column="1">
												<StackPanel Orientation="Vertical" Margin="0,70,0,0" Spacing="15" HorizontalAlignment="Right">
													<StackPanel Spacing="5" Margin="0,4,0,0">
														<TextBlock
															Text="{CompiledBinding CurrentDate}"
															TextAlignment="Right"
															Classes="h5" />
														<TextBlock
															Text="{CompiledBinding CurrentTime}"
															TextAlignment="Right"
															Classes="h5" />
													</StackPanel>
												</StackPanel>
											</StackPanel>
										</Grid>
										<Separator Background="{StaticResource ForegroundColor}"
										           IsVisible="{CompiledBinding IsPaymentPossible}" />
										<Grid RowDefinitions="Auto" ColumnDefinitions="* *"
										      IsVisible="{CompiledBinding IsPaymentPossible}">
											<!-- Left Column -->
											<StackPanel Grid.Column="0" Spacing="0">
												<StackPanel Orientation="Horizontal" Margin="0,30" Spacing="15">
													<TextBlock
														Classes="Muted"
														FontSize="19"
														Text="Purchase Summary"
														VerticalAlignment="Center" />
												</StackPanel>
												<StackPanel Orientation="Vertical" Margin="0,-25,0,20" Spacing="10">
													<StackPanel Orientation="Vertical" Spacing="5" Margin="0,15">
														<TextBlock
															Text="{CompiledBinding MembershipDurationQuantitySummary}"
															Classes="h5" />
														<TextBlock
															Text="Total:"
															Classes="h5" />
														<TextBlock
															Text="Tendered Price:"
															Classes="h5" />
														<TextBlock
															Text="Change:"
															Classes="h5" />
													</StackPanel>
												</StackPanel>
											</StackPanel>

											<!-- Right Column -->
											<StackPanel Grid.Column="1">
												<StackPanel Orientation="Vertical" Margin="0,70,0,0" Spacing="15"
												            HorizontalAlignment="Right">
													<StackPanel Spacing="5" Margin="0,4,0,0">
														<TextBlock
															Text="{CompiledBinding SubtotalDisplay}"
															TextAlignment="Right"
															Classes="h5" />
														<TextBlock
															Text="{CompiledBinding PurchasedTenderedPrice}"
															Classes="h5" />
														<TextBlock
															Text="{CompiledBinding PurchaseChange}"
															TextAlignment="Right"
															Classes="h5" />
													</StackPanel>
												</StackPanel>
											</StackPanel>
										</Grid>
										<Separator
											Background="{StaticResource ForegroundColor}"
											IsVisible="{CompiledBinding IsPaymentPossible}" />
										<Grid RowDefinitions="Auto Auto Auto" ColumnDefinitions="4* 1.8*"
										      IsVisible="{CompiledBinding IsPaymentPossible}">
											<StackPanel Grid.Column="0" Spacing="0">
												<StackPanel Orientation="Horizontal" Margin="0,20" Spacing="15">
													<TextBlock
														Classes="h2"
														FontWeight="Bold"
														FontSize="25"
														Text="Change:"
														VerticalAlignment="Center" />
												</StackPanel>
											</StackPanel>
											<StackPanel Grid.Row="0" Grid.Column="1">
												<StackPanel Orientation="Vertical" Margin="0,15,0,0" Spacing="15"
												            HorizontalAlignment="Right">
													<StackPanel Spacing="5" Margin="0,4,0,0">
														<TextBlock
															Text="{CompiledBinding PurchaseChange}"
															Foreground="{DynamicResource SuccessColor}"
															Classes="h2" />
													</StackPanel>
												</StackPanel>
											</StackPanel>
										</Grid>
										<Grid RowDefinitions="Auto Auto" IsVisible="{CompiledBinding IsPaymentPossible}">
											<StackPanel Grid.Row="0" Margin="0,10">
												<shadui:Card
													Background="{DynamicResource InfoColor10}"
													BorderThickness="0">
													<StackPanel>
														<StackPanel Orientation="Horizontal" Spacing="15">
															<Svg
																Path="avares://AHON_TRACK/Assets/CheckInOutView/clock-icon.svg"
																Width="26"
																Height="26"
																HorizontalAlignment="Left"
																VerticalAlignment="Center" />
															<TextBlock
																Text="Membership Validity"
																FontSize="20"
																FontWeight="SemiBold"
																HorizontalAlignment="Left"
																VerticalAlignment="Center"
																Foreground="{DynamicResource InfoColor}" />
														</StackPanel>
														<StackPanel>
															<TextBlock
																Text="{CompiledBinding ValidityFrom}"
																Foreground="{DynamicResource InfoColor}"
																FontSize="18"
																FontWeight="Medium"
																Margin="0,20" />
															<TextBlock
																Text="{CompiledBinding ValidityTo}"
																Foreground="{DynamicResource InfoColor}"
																FontSize="18"
																FontWeight="Medium" />
														</StackPanel>
													</StackPanel>
												</shadui:Card>
											</StackPanel>
											<StackPanel Grid.Row="1">
												<shadui:Card
													Background="{DynamicResource PrimaryColor10}"
													BorderThickness="0">
													<StackPanel>
														<TextBlock
															Text="Important Notes:"
															Classes="Muted"
															FontSize="20"
															FontWeight="SemiBold" />
														<TextBlock
															Text="• Membership is not transferable"
															Classes="Muted"
															FontSize="18"
															FontWeight="Medium"
															Margin="30,10" />
														<TextBlock
															Text="• No refunds for used/unused sessions"
															Classes="Muted"
															FontSize="18"
															FontWeight="Medium"
															Margin="30,0" />
													</StackPanel>
												</shadui:Card>
											</StackPanel>
										</Grid>
									</StackPanel>
									<Button
										Content="{CompiledBinding GrandTenderedPrice}"
										Classes="Primary"
										FontSize="20"
										Height="50"
										HorizontalAlignment="Right"
										Margin="0,20,0,0"
										Padding="30,0"
										IsVisible="{CompiledBinding IsPaymentPossible}"
										IsEnabled="{CompiledBinding IsPaymentPossible}"
										Command="{CompiledBinding PaymentCommand}" />
								</StackPanel>
							</shadui:Card>
						</Grid>
					</shadui:BreakpointView>
				</shadui:BreakpointViewPort>
			</StackPanel>
		</ScrollViewer>
	</DockPanel>
</UserControl>